@article{xu2007,
  title = {Bacterial {{Load Predicts Healing Rate}} in {{Neuropathic Diabetic Foot Ulcers}}},
  author = {Xu, Ling and McLennan, Susan V. and Lo, Lisa and Natfaji, Anas and Bolton, Thyra and Liu, Yu and Twigg, Stephen M. and Yue, Dennis K.},
