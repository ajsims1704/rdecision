@article{liddell1983,
  title = {Simplified Exact Analysis of Case-Referent Studies: Matched Pairs; Dichotomous Exposure.},
  author = {Liddell, F D},
  date = {1983-03},
  journaltitle = {Journal of epidemiology and community health},
  volume = {37},
  pages = {82--4},
  issn = {0143-005X},
  url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1052263&tool=pmcentrez&rendertype=abstract},
  abstract = {In a case-referent study of matched pairs with a single dichotomous exposure variable, the relative risk R is estimated from the "discordant pairs," r and s, by R = r/s. In this paper the exact (1 - alpha) confidence limits, RL and RU, have been simplified, and a new exact test of the null hypothesis that R = 1 has been derived. The exact methods depend on the F distribution; they are simple to calculate and all have been fully validated. Even after simplification, methods for obtaining approximations to RL and RU remain more unwieldy than the exact methods, and an evaluation shows that these approximations may occasionally be far from the truth. It is argued that there is no excuse for not using the exact methods.},
  eprint = {6875452},
  eprinttype = {pmid},
  keywords = {Epidemiologic Methods,Risk,Statistics as Topic},
  number = {1}
}

