@article{bartone2008,
  title = {Comparison of {{Ultrafiltration}}, {{Nesiritide}}, and {{Usual Care}} in {{Acute Decompensated Heart Failure}}},
  author = {Bartone, Cheryl and Saghir, Syed and Menon, Santosh G. and Brosmer, Jayna and Kereiakes, Dean J. and Mazur, Wojciech and Chung, Eugene S.},
  date = {2008-11-01},
  journaltitle = {Congestive Heart Failure},
