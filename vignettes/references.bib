@article{khan2008,
  title = {Critical Review of the Ankle Brachial Index.},
  author = {Khan, Tahir H and a Farooqui, Falahat and Niazi, Khusrow},
  date = {2008-05},
  journaltitle = {Current cardiology reviews},
  volume = {4},
