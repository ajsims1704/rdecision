@article{chapman2009,
  title = {Clinical Efficacy and Cost-Effectiveness of Outpatient Parenteral Antibiotic Therapy ({{OPAT}}): {{A UK}} Perspective},
  author = {Chapman, Ann L N and Dixon, Simon and Andrews, Dawn and Lillie, Patrick J. and Bazaz, Rohit and Patchett, Julie D.},
  date = {2009},
  journaltitle = {Journal of Antimicrobial Chemotherapy},
  volume = {64},
  pages = {1316--1324},
  issn = {03057453},
  doi = {10.1093/jac/dkp343},
  abstract = {OBJECTIVES: Outpatient parenteral antibiotic therapy (OPAT) is an effective treatment strategy for a wide variety of infections as long as clinical risk is minimized by conforming to practice guidelines. However, its cost-effectiveness has not been established in the setting of the UK National Health Service. We examined the clinical efficacy and cost-effectiveness of an OPAT service based in a large UK teaching hospital, predominantly using the outpatient 'infusion centre' and patient/carer administration models of service delivery.\$\textbackslash backslash\$n\$\textbackslash backslash\$nPATIENTS AND METHODS: Data on clinical activity and outcomes were collected prospectively on 334 episodes of treatment administered by the Sheffield OPAT service between January 2006 and January 2008. Cost-effectiveness was calculated by comparing real costs of OPAT with estimated inpatient costs for these patient episodes incorporating two additional sensitivity analyses.\$\textbackslash backslash\$n\$\textbackslash backslash\$nRESULTS: Of the OPAT episodes, 87\% resulted in cure or improvement on completion of intravenous therapy. The readmission rate was 6.3\%, and patient satisfaction was high. OPAT cost 41\% of equivalent inpatient costs for an Infectious Diseases Unit, 47\% of equivalent inpatient costs using national average costs and 61\% of inpatient costs using minimum inpatient costs for each diagnosis.\$\textbackslash backslash\$n\$\textbackslash backslash\$nCONCLUSIONS: Using this service model, OPAT is safe and clinically effective, with low rates of complications/readmissions and high levels of patient satisfaction. OPAT is cost-effective when compared with equivalent inpatient care in the UK healthcare setting.},
  eprint = {19767623},
  eprinttype = {pmid},
  isbn = {1460-2091 (Electronic)\$\textbackslash backslash\$r0305-7453 (Linking)},
  keywords = {Cost analysis,Home infusion therapy,Intravenous antibiotics,Quality assurance},
  number = {6}
}

