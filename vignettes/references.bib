  isbn = {1097-0258},
  keywords = {Deterministic sensitivity analysis,Evidence synthesis,Generalized meta-analysis,Markov chain Monte Carlo simulation,Probabilistic sensitivity analysis,WinBUGS},
  number = {23}
}

