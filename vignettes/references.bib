@article{airainer1993,
  title = {Quantitative Evaluation of Phonetograms in the Case of Functional Dysphonia},
  author = {Airainer, R. and Klingholz, F.},
  date = {1993-06-01},
  journaltitle = {Journal of Voice},
  shortjournal = {Journal of Voice},
  volume = {7},
