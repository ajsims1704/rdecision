@article{clark2003a,
  title = {Peripheral {{Arterial Disease}} in {{People With Diabetes}}},
  author = {Clark, Nathaniel},
  date = {2003-12-01},
  journaltitle = {Diabetes Care},
  shortjournal = {Diabetes Care},
