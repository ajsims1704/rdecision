@article{harrison2019,
  title = {Formal Techniques in the Safety Analysis of Software Components of a New Dialysis Machine},
  author = {Harrison, Michael D. and Freitas, Leo and Drinnan, Michael and Campos, José C. and Masci, Paolo and di Maria, Costanzo and Whitaker, Michael},
  date = {2019-04-15},
  journaltitle = {Science of Computer Programming},
  shortjournal = {Science of Computer Programming},
  volume = {175},
  pages = {17--34},
  issn = {0167-6423},
  doi = {10.1016/j.scico.2019.02.003},
  url = {http://www.sciencedirect.com/science/article/pii/S0167642318300819},
  abstract = {The paper is concerned with the practical use of formal techniques to contribute to the risk analysis of a new neonatal dialysis machine. The described formal analysis focuses on the controller component of the software implementation. The controller drives the dialysis cycle and deals with error management. The logic was analysed using model checking techniques and the source code was analysed formally, checking type correctness conditions, use of pointers and shared memory. The analysis provided evidence of the verification of risk control measures relating to the software component. The productive dialogue between the developers of the device, who had no experience or knowledge of formal methods, and the analyst using the formal analysis tools, provided a basis for the development of rationale for the effectiveness of the evidence.},
  keywords = {Formal methods,Haemodialysis,Medical devices,Model checking,Risk analysis},
  options = {useprefix=true}
}

@article{harron2017,
  title = {Challenges in Administrative Data Linkage for Research},
  author = {Harron, Katie and Dibben, Chris and Boyd, James and Hjern, Anders and Azimaee, Mahmoud and Barreto, Mauricio L and Goldstein, Harvey},
  date = {2017-12-01},
  journaltitle = {Big Data \& Society},
  shortjournal = {Big Data \& Society},
  volume = {4},
  pages = {2053951717745678},
  publisher = {{SAGE Publications Ltd}},
  issn = {2053-9517},
  doi = {10.1177/2053951717745678},
  url = {https://doi.org/10.1177/2053951717745678},
  urldate = {2021-01-20},
  abstract = {Linkage of population-based administrative data is a valuable tool for combining detailed individual-level information from different sources for research. While not a substitute for classical studies based on primary data collection, analyses of linked administrative data can answer questions that require large sample sizes or detailed data on hard-to-reach populations, and generate evidence with a high level of external validity and applicability for policy making. There are unique challenges in the appropriate research use of linked administrative data, for example with respect to bias from linkage errors where records cannot be linked or are linked together incorrectly. For confidentiality and other reasons, the separation of data linkage processes and analysis of linked data is generally regarded as best practice. However, the ?black box? of data linkage can make it difficult for researchers to judge the reliability of the resulting linked data for their required purposes. This article aims to provide an overview of challenges in linking administrative data for research. We aim to increase understanding of the implications of (i) the data linkage environment and privacy preservation; (ii) the linkage process itself (including data preparation, and deterministic and probabilistic linkage methods) and (iii) linkage quality and potential bias in linked data. We draw on examples from a number of countries to illustrate a range of approaches for data linkage in different contexts.},
  number = {2}
}

@article{harron2017a,
  title = {A Guide to Evaluating Linkage Quality for the Analysis of Linked Data},
  author = {Harron, Katie L and Doidge, James C and Knight, Hannah E and Gilbert, Ruth E and Goldstein, Harvey and Cromwell, David A and van der Meulen, Jan H},
  date = {2017-10-01},
  journaltitle = {International Journal of Epidemiology},
  shortjournal = {International Journal of Epidemiology},
  volume = {46},
  pages = {1699--1710},
  issn = {0300-5771},
  doi = {10.1093/ije/dyx177},
  url = {https://doi.org/10.1093/ije/dyx177},
  urldate = {2021-01-20},
  abstract = {Linked datasets are an important resource for epidemiological and clinical studies, but linkage error can lead to biased results. For data security reasons, linkage of personal identifiers is often performed by a third party, making it difficult for researchers to assess the quality of the linked dataset in the context of specific research questions. This is compounded by a lack of guidance on how to determine the potential impact of linkage error. We describe how linkage quality can be evaluated and provide widely applicable guidance for both data providers and researchers. Using an illustrative example of a linked dataset of maternal and baby hospital records, we demonstrate three approaches for evaluating linkage quality: applying the linkage algorithm to a subset of gold standard data to quantify linkage error; comparing characteristics of linked and unlinked data to identify potential sources of bias; and evaluating the sensitivity of results to changes in the linkage procedure. These approaches can inform our understanding of the potential impact of linkage error and provide an opportunity to select the most appropriate linkage procedure for a specific analysis. Evaluating linkage quality in this way will improve the quality and transparency of epidemiological and clinical research using linked data.},
  number = {5},
  options = {useprefix=true}
}

