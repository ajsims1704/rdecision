@article{maltais2013,
  title = {Episodic Monoplane Transesophageal Echocardiography Impacts Postoperative Management of the Cardiac Surgery Patient.},
  author = {Maltais, Simon and Costello, William T and Billings, Frederic T and Bick, Julian S and Byrne, John G and Ahmad, Rashid M and Wagner, Chad E},
